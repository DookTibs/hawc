    "redux-devtools-log-monitor": "^1.0.1",
    "redux-logger": "^2.3.1",
    "redux-mock-store": "0.0.6",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "sortablejs": "^1.4.2",
    "style-loader": "^0.13.0",
    "tablesort": "^4.1.0",
    "underscore": "^1.8.3",
    "unexpected": "^10.13.2",
    "unexpected-react": "^3.2.1",
    "webpack": "^1.13.0",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
