{
  "name": "react-HAWC-endpoint-cleanup",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "npm run build:dev && node devServer.js",
    "test": "mocha ./assets/js/test/*.js --compilers js:babel-core/register --require ./assets/js/test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint assets"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "CC0-1.0",
  "homepage": "https://github.com/gaearon/react-transform-boilerplate",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "exports-loader": "^0.6.2",
    "express": "^4.13.3",
    "history": "^1.13.1",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "lodash": "^3.10.1",
    "mocha": "^2.3.4",
    "normalizr": "^1.4.0",
    "react-redux": "^4.0.1",
    "react-router": "^1.0.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
    "redux": "^3.0.5",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-logger": "^2.3.1",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.2",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "whatwg-fetch": "^0.10.1"
  },
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  }
}
