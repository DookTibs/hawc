{
  "name": "react-HAWC-endpoint-cleanup",
  "version": "1.0.0",
  "description": "React app for modifying model fields in bulk.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "npm run build:dev && node devServer.js",
    "test": "env NODE_PATH=$NODE_PATH:$PWD/assets mocha ./assets/tests/ --compilers js:babel-core/register --require ./assets/tests/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint assets"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shapiromatron/hawc.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "webpack",
    "babel",
    "react-transform"
  ],
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.0",
    "d3": "^3.5.17",
    "enzyme": "^2.3.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "exports-loader": "^0.6.2",
    "express": "^4.13.3",
    "filesaver.js": "^0.2.0",
    "history": "^1.13.1",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "mocha": "^2.3.4",
    "moment": "^2.11.1",
    "nock": "^5.2.1",
    "quill": "^0.20.1",
    "react-addons-test-utils": "^15.0.2",
    "react-faux-dom": "^2.2.0",
    "react-mixin-sortablejs": "^0.1.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
